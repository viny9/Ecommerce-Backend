generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id
  name String 
  email String
  phone String
  is_admin Boolean
  stripe_id String
  address_id String
  card_id String
  created_at DateTime
  deleted_at DateTime
  password String
}
